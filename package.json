{
  "name": "template-meta-processor",
  "version": "1.0.0",
  "description": "Production scripts for processing meta.json - remove duplicates and sort alphabetically",
  "main": "build-scripts/process-meta.js",
  "scripts": {
    "process-meta": "node dedupe-and-sort-meta.js && prettier --write meta.json",
    "process-meta-only": "node dedupe-and-sort-meta.js",
    "process-meta-verbose": "node build-scripts/process-meta.js --verbose && prettier --write meta.json",
    "process-meta-with-backup": "node build-scripts/process-meta.js --backup && prettier --write meta.json",
    "validate-meta": "node build-scripts/process-meta.js --no-backup --verbose",
    "build": "npm run process-meta",
    "prebuild": "echo 'Processing meta.json for production build...'",
    "postbuild": "echo 'Meta.json processing completed!'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:app": "cd app && npm run format",
    "format:check:app": "cd app && npm run format:check"
  },
  "keywords": [
    "meta",
    "json",
    "dedupe",
    "sort",
    "build",
    "production"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.0",
    "prettier": "^3.2.5"
  }
}
