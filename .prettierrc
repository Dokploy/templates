{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "proseWrap": "preserve",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto",
  "singleAttributePerLine": false,
  "overrides": [
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
      "options": {
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 100,
        "bracketSpacing": true,
        "arrowParens": "avoid"
      }
    },
    {
      "files": ["*.d.ts"],
      "options": {
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5",
        "printWidth": 120,
        "bracketSpacing": true,
        "arrowParens": "avoid"
      }
    },
    {
      "files": ["*.html", "*.htm"],
      "options": {
        "printWidth": 120,
        "singleQuote": false,
        "htmlWhitespaceSensitivity": "css",
        "singleAttributePerLine": false,
        "bracketSameLine": false,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.css", "*.scss", "*.sass", "*.less"],
      "options": {
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "none",
        "tabWidth": 2
      }
    },
    {
      "files": ["*.json", "*.jsonc"],
      "options": {
        "printWidth": 120,
        "trailingComma": "none",
        "singleQuote": false,
        "tabWidth": 2
      }
    },
    {
      "files": ["package.json", "package-lock.json", "tsconfig.json", "tsconfig.*.json"],
      "options": {
        "printWidth": 120,
        "trailingComma": "none",
        "singleQuote": false,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.yaml", "*.yml"],
      "options": {
        "printWidth": 120,
        "singleQuote": false,
        "tabWidth": 2,
        "bracketSpacing": true
      }
    },
    {
      "files": ["docker-compose.yml", "docker-compose.*.yml", ".github/workflows/*.yml"],
      "options": {
        "printWidth": 120,
        "singleQuote": false,
        "tabWidth": 2,
        "bracketSpacing": true
      }
    },
    {
      "files": ["*.toml"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    },
    {
      "files": ["template.toml", "Cargo.toml", "pyproject.toml"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.svg"],
      "options": {
        "printWidth": 120,
        "htmlWhitespaceSensitivity": "ignore",
        "singleAttributePerLine": false,
        "bracketSameLine": true,
        "singleQuote": false
      }
    },
    {
      "files": ["*.xml"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2,
        "singleAttributePerLine": true,
        "bracketSameLine": false,
        "singleQuote": false
      }
    },
    {
      "files": ["*.config.js", "*.config.ts", "*.config.mjs", "*.config.cjs"],
      "options": {
        "printWidth": 100,
        "singleQuote": true,
        "trailingComma": "es5",
        "semi": true
      }
    },
    {
      "files": ["vite.config.*", "webpack.config.*", "rollup.config.*", "next.config.*"],
      "options": {
        "printWidth": 100,
        "singleQuote": true,
        "trailingComma": "es5",
        "semi": true
      }
    },
    {
      "files": [".prettierrc", ".eslintrc", ".babelrc", ".stylelintrc"],
      "options": {
        "printWidth": 80,
        "trailingComma": "none",
        "singleQuote": false,
        "tabWidth": 2
      }
    },
    {
      "files": [".eslintrc.js", ".eslintrc.ts", ".eslintrc.json"],
      "options": {
        "printWidth": 100,
        "trailingComma": "es5",
        "singleQuote": true,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.dockerfile", "Dockerfile*"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.sh", "*.bash", "*.zsh"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    },
    {
      "files": ["Makefile", "makefile"],
      "options": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": true
      }
    },
    {
      "files": ["*.env", ".env.*"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    },
    {
      "files": [".gitignore", ".dockerignore", ".npmignore"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.graphql", "*.gql"],
      "options": {
        "printWidth": 100,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.vue"],
      "options": {
        "printWidth": 100,
        "singleQuote": true,
        "semi": true,
        "trailingComma": "es5",
        "tabWidth": 2
      }
    },
    {
      "files": ["*.php"],
      "options": {
        "printWidth": 120,
        "singleQuote": true,
        "semi": true,
        "trailingComma": "es5",
        "tabWidth": 4
      }
    },
    {
      "files": ["*.py"],
      "options": {
        "printWidth": 88,
        "tabWidth": 4
      }
    },
    {
      "files": ["*.rb"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.go"],
      "options": {
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": true
      }
    },
    {
      "files": ["*.rs"],
      "options": {
        "printWidth": 100,
        "tabWidth": 4
      }
    },
    {
      "files": ["*.java"],
      "options": {
        "printWidth": 120,
        "tabWidth": 4
      }
    },
    {
      "files": ["*.c", "*.cpp", "*.h", "*.hpp"],
      "options": {
        "printWidth": 120,
        "tabWidth": 4
      }
    },
    {
      "files": ["*.sql"],
      "options": {
        "printWidth": 120,
        "tabWidth": 2
      }
    }
  ]
}
